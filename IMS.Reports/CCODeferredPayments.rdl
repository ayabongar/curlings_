        <CommandText>Declare @tblCenter  table
( 
Incidentid nvarchar(100), 
[TimeStamp] nvarchar(100),
[DateCompleted]  nvarchar(100),
[CentreWhereAgentIsBased] nvarchar(100),
[BusinessAreaManager] nvarchar(100),
[DirectTeamLeader]  nvarchar(100),
[RelaventTaxNumber]  nvarchar(100),
[ServiceManagerCaseNo]  nvarchar(100),
[RequestedDeferredAmountOutstanding] nvarchar(100),
[DeferredPeriod]  nvarchar(100),
[StatusOfRequest]  nvarchar(100),
[IfDeclinedReasons]  nvarchar(100)
)

Declare @tblManagers  table
(
[CentreWhereAgentIsBased ] nvarchar(100),
[Manager(YTD)] nvarchar(100),
[OPS Manager(YTD)] nvarchar(100),
[OPS Manager(YTD)SID]  nvarchar(100),
[Total Deferment (YTD)] int,
[AVTime] int
)

Declare @tblTotalApproved  table
( 
[CentreWhereAgentIsBased ] nvarchar(100),
[DirectTeamLeader ]  nvarchar(100),
[Total deferment amount approved (YTD) ] int
)

Declare @tblTotalAwaitingApproval  table
(  
[CentreWhereAgentIsBased ] nvarchar(100),
[DirectTeamLeader ]  nvarchar(100),
[Total deferment  amount waiting to be approved (YTD)] int
)
Declare @tblTotalDeclined  table
( 
[CentreWhereAgentIsBased ] nvarchar(100),
[DirectTeamLeader ]  nvarchar(100),
[Total amount deferment declined (YTD)] int
)

 insert Into @tblCenter
 EXECUTE uspPivot_READ_ProcessData @ProcessId
  
   INSERT INTO @tblManagers   
      SELECT
	     [CentreWhereAgentIsBased],
		 Manager.FullName as [Manager(YTD)],
		 OpsManager.FullName as [OPS Manager(YTD)],
		 [DirectTeamLeader] as [OPS Manager(YTD)SID],
		 SUM(convert(decimal(18,2),[RequestedDeferredAmountOutstanding])) as [Total Deferment (YTD)] ,
		 AVG(datediff(Day,a.TimeStamp,a.DateCompleted)) as [AVTime] 
	  FROM @tblCenter a inner join Users Manager on a.[BusinessAreaManager] = Manager.SID
		 Inner Join Users OpsManager on a.[DirectTeamLeader] = OpsManager.SID
		 WHERE a.TimeStamp between @StartDate and @EndDate
	  GROUP BY a.[CentreWhereAgentIsBased],[BusinessAreaManager],Manager.FullName,OpsManager.FullName , [DirectTeamLeader]	 
	


 INSERT INTO @tblTotalApproved 
	 SELECT   [CentreWhereAgentIsBased],
			  [DirectTeamLeader],
			   SUM(convert(decimal(18,2),[RequestedDeferredAmountOutstanding])) as [Total deferment amount approved (YTD)]
	 FROM @tblCenter a inner join Users Manager on a.[BusinessAreaManager] = Manager.SID
	 Inner Join Users OpsManager on a.[DirectTeamLeader] = OpsManager.SID
	 where [StatusOfRequest] = 'Approved'
	 Group by a.[CentreWhereAgentIsBased],[BusinessAreaManager],Manager.FullName,OpsManager.FullName , [DirectTeamLeader]	

INSERT INTO @tblTotalAwaitingApproval
	 SELECT   [CentreWhereAgentIsBased],					 
			  [DirectTeamLeader] as [OPS Manager(YTD)SID],
			   SUM(convert(decimal(18,2),[RequestedDeferredAmountOutstanding])) as [Total deferment  amount waiting to be approved (YTD)]
	 FROM @tblCenter a inner join Users Manager on a.[BusinessAreaManager] = Manager.SID
	 Inner Join Users OpsManager on a.[DirectTeamLeader] = OpsManager.SID
	 where [StatusOfRequest] = 'Awaiting Approval'
	 Group by a.[CentreWhereAgentIsBased],[BusinessAreaManager],Manager.FullName,OpsManager.FullName , [DirectTeamLeader]

INSERT INTO @tblTotalDeclined
	 SELECT   [CentreWhereAgentIsBased],			 
			  [DirectTeamLeader] as [OPS Manager(YTD)SID],
			  SUM(convert(decimal(18,2),[RequestedDeferredAmountOutstanding])) as [Total amount deferment declined (YTD)]
	 FROM @tblCenter a inner join Users Manager on a.[BusinessAreaManager] = Manager.SID
	 Inner Join Users OpsManager on a.[DirectTeamLeader] = OpsManager.SID
	 where [StatusOfRequest] = 'Declined'
	 Group by a.[CentreWhereAgentIsBased],[BusinessAreaManager],Manager.FullName,OpsManager.FullName , [DirectTeamLeader]


	  SELECT  m.[CentreWhereAgentIsBased ],
		      m.[Manager(YTD)],
		      m.[OPS Manager(YTD)],			 
			  [Total deferment  amount waiting to be approved (YTD)],
			  [Total amount deferment declined (YTD)],
			  [Total deferment amount approved (YTD) ],
			  [Total Deferment (YTD)],
			  [AVTime] 
    FROM @tblManagers m
	        LEFT JOIN @tblTotalApproved a on m.[OPS Manager(YTD)SID] = a.[DirectTeamLeader ] 
			LEFT JOIN @tblTotalAwaitingApproval aw on m.[OPS Manager(YTD)SID] = aw.[DirectTeamLeader ] 
			LEFT JOIN @tblTotalDeclined d on m.[OPS Manager(YTD)SID] = d.[DirectTeamLeader ]</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
