        <CommandText>declare @tsql nvarchar(2000)
  
   IF @col2 &lt;&gt; 'None'
    BEGIN
        SET @tsql = 'SELECT [' +@col1 + '],[' + @col2+ '],Count(1) as Total FROM [dbo].[vw_PCM] pcm
           inner join [dbo].[Incidents] inc
